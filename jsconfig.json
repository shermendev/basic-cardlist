{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "baseUrl": "./src/",
    "paths": {
      "@@components/*": ["./components/*"],
      "@@containers/*": ["./containers/*"],
      "@@hoc/*": ["./hoc/*"],
      "@@icons/*": ["./assets/img/icons/*"],
      "@@img/*": ["./assets/img/*"],
      "@@pages/*": ["./pages/*"],
      "@@renderProps/*": ["./render-props/*"],
      "@@store/*": ["./store/*"],
      "@@styles/*": ["./styles/*"],
      "@@utils/*": ["./utils/*"]
    }
  },
  "include": ["src/**/*"]
}
