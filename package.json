{
  "name": "cardboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://shermendev.gitlab.io/cardboard",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@reduxjs/toolkit": "^1.2.5",
    "@styled-icons/fa-solid": "^9.3.0",
    "@styled-icons/icomoon": "^9.3.0",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "broadcast-channel": "^3.0.4",
    "connected-react-router": "^6.7.0",
    "css-checkbox-library": "^1.4.0",
    "fast-deep-equal": "^3.1.1",
    "history": "^4.10.1",
    "localforage": "^1.7.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-countdown-now": "^2.1.2",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^4.9.6",
    "react-hot-loader": "^4.12.19",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.0.1",
    "styled-map": "^3.3.0"
  },
  "scripts": {
    "analyze": "craco build --config config/craco.config.analyze.js",
    "build": "craco build --config config/craco.config.build.js",
    "eject": "react-scripts eject",
    "fix:obj": "jscodeshift -t node_modules/codemods/transforms/sort-object-props.js",
    "start": "craco start --config config/craco.config.js",
    "sass-lint": "sass-lint -v -q --max-warnings=0",
    "sass-lint:fix": "sass-lint-auto-fix",
    "test": "craco test",
    "postinstall": "link-module-alias"
  },
  "_moduleAliases": {
    "~root": "./src",
    "~components": "src/components",
    "~config": "src/config",
    "~hooks": "src/hooks",
    "~icons": "src/assets/img/icons",
    "~img": "src/assets/img",
    "~pages": "src/pages",
    "~store": "src/store",
    "~styles": "src/styles",
    "~utils": "src/utils"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@craco/craco": "^5.6.3",
    "@welldone-software/why-did-you-render": "^4.0.4",
    "babel-plugin-styled-components": "^1.10.7",
    "codemods": "shermendev/codemods#0.8.1",
    "craco-favicons": "^1.0.0",
    "craco-plugin-react-hot-reload": "^0.1.0",
    "directory-named-webpack-plugin": "^4.0.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-unicorn": "^16.1.1",
    "husky": "^4.2.3",
    "jscodeshift": "^0.7.0",
    "link-module-alias": "^1.2.0",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.0",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
